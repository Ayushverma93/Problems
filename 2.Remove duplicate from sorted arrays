//---------------------Remove duplicate from sorted Array--------------------
class Solution {
    public int removeDuplicates(int[] nums) {
        // We use two pointer methods first use a temp value and second use for compare and store. 
        int j=0;
        for(int i=0; i<nums.length; i++){
            if(nums[j] != nums[i]){
                nums[++j]=nums[i];
            }
        }
        return ++j;
    }
}